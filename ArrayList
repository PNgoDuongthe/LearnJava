import java.util.ArrayList;
class Arraylist {
    public static void main(String[] args) {
        ArrayList<String> food = new ArrayList<String>();
        food.add("Banh mi");
        food.add("Com tam");
        food.add("Bun cha");
//        food.remove(1);
//        food.clear();
//        food.set(0,"cha ca ");
        for(int i =0;i< food.size();i++)
        {
            System.out.println(food.get(i));
        }
    }
}
